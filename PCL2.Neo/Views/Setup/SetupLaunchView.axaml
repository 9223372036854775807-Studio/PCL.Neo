<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vmd="clr-namespace:PCL2.Neo.ViewModels.Setup"
             xmlns:pc="using:PCL2.Neo.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="PCL2.Neo.Views.Setup.SetupLaunchView"
             x:DataType="vmd:SetupLaunchViewModel">
    <Design.DataContext>
        <vmd:SetupLaunchViewModel />
    </Design.DataContext>
    <ScrollViewer>
        <StackPanel Orientation="Vertical" Margin="20" Spacing="16">
            <pc:MyCard Title="启动选项" Height="80"></pc:MyCard>
            <pc:MyCard Title="游戏内存" Height="80"></pc:MyCard>
            <pc:MyButton Text="搜索Java" Height="40"></pc:MyButton>
        </StackPanel>
    </ScrollViewer>
</UserControl>
